{
  "name": "react-scaffold",
  "version": "1.0.0",
  "description": "react-scaffold",
  "main": "index.js",
  "scripts": {
    "dev_server": "npm run dev&&npm run server_dev",
    "pro_server": "npm run mkdir_pro_dir && npm run pro && npm run server_pro ",
    "mkdir_pro_dir": "node ./bin/copy",
    "server_dev": "set NODE_ENV=development&&node ./bin/www",
    "server_pro": "set NODE_ENV=production&&babel ./server -d ./build/nodeServer/server&&babel ./src -d ./build/nodeServer/src &&node ./bin/www",
    "dev": "webpack -p --progress --profile --config   webpack-dev-config.js",
    "pro": "set NODE_ENV=production&&webpack -p --progress --profile --config webpack-pro-config.js"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.24.1",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "open-browser-webpack-plugin": "0.0.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "asset-require-hook": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "css-modules-require-hook": "^4.0.6",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "handlebars": "^4.0.10",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
